    <span style="color:Blue;">ByVal</span> e <span style="color:Blue;">As</span> EventArgs) <span style="color:Blue;">Handles</span> printPreviewButton.Click

    ReadDocument()
    printPreviewDialog1.Document = printDocument1
    printPreviewDialog1.ShowDialog()
<span style="color:Blue;">End</span>&nbsp;<span style="color:Blue;">Sub</span>
