/* Gmap travel modes */

.cb-drive, .cb-bike, .cb-walk, .cb-drive span, .cb-bike span, .cb-walk span { background: url("/img/travel_modes.png") repeat-x; display: block; float: left; width: 37px; height: 23px;}

.cb-drive { border: 1px solid #ccc; }
.cb-bike { border-top: 1px solid #ccc; border-bottom: 1px solid #ccc; border-right: 1px solid #ccc; }
.cb-walk { border-top: 1px solid #ccc; border-right: 1px solid #ccc; border-bottom: 1px solid #ccc; }

.cb-drive span, .cb-walk span, .cb-bike span { line-height: 23px; display: block; background-repeat: no-repeat; font-weight: bold; text-indent: -9999em; }

.cb-drive span { background-position: left 0; padding: 0 10px; }
.cb-bike span { background-position: left -69px; padding: 0 10px; }
.cb-walk span { background-position: left -46px; padding: 0 10px; }

.cb-drive.selected { background-position: left -23px; }
.cb-drive.selected span { background-position: left -23px; color: #fff; }

.cb-bike.selected { background-position: left -115px; }
.cb-bike.selected span { background-position: left -115px; color: #fff; }

.cb-walk.selected { background-position: left -92px; }
.cb-walk.selected span { background-position: left -92px; color: #fff; }
	
.switch label { cursor: pointer; }
.switch input { display: none; }

.field { clear: both; width: 115px; height: 30px; margin: 0 auto 12px auto; }

