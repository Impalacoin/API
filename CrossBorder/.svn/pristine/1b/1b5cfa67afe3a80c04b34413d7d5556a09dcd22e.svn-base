/* HEADER */

#header {
  background: #fff;
  height: 53px;  
  min-width: 1100px;
  position: relative;
}

#masthead {
  margin: 0 auto;
  width: 1046px;
}

#masthead ul {
  float: right;
  list-style: none;
  /*margin-right: 17px;*/margin-right: 0;
