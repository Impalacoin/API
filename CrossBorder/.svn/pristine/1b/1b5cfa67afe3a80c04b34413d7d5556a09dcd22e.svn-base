/* LAYOUT */

html {
  height: 100%;
}

body {
  background: #e4e4e5;
  color: #636466;
  height: 100%;
  font-family: "Muli", Arial;
  max-width: 100%;
}


/* TEXT */

h1, h2, h3, h4, h5, h6, h7 {
  color: #781d7e;
  font-weight: normal;
  margin: 0;
  text-transform: uppercase;
}

h2 {
  font-size: 13.5px;
}

.sub h1,
#main .col h2,
#content.media-gallery #main h2 {
  font-size: 25px;
  color: #636466;
  text-transform: none;
  margin-bottom: 30px;
  font-weight: normal;
}

.sub h1 a,
#main .col h2 a,
#content.media-gallery #main h2 a {
  color: #636466;
  text-decoration: none;
}

#main .col h2,
.more #main .col h2,
.events #content.landing #main .col h2,
#content.media-gallery #main h2 {
  font-size: 22px;
}

/*.newsletters #main .col h2 {
  margin-bottom: 15px;
}*/

.newsletters #main .col h3 {
  margin: 30px 0 0 0;
}

.sub.news .wrap .col h3 {
	padding-top: 15px;
}

.sub.media .wrap .col h3 {
	margin-bottom: 20px;
}

.sub.media .wrap .col h4 {
	margin: 10px 0 5px 0;
	font-size: 12px;
	font-weight: bold;
}

.newsletters #main .col h3:first-child {
  margin-top: 15px;
}

.sub.photo-gallery #main .wrap {
  background: none;
}

.sub.photo-gallery #main .wrap .col ul {
  margin: 0;
  padding: 0;
  list-style: none;
}

.sub.photo-gallery #main .wrap .col ul li {
  margin: 8px 0;
}

.sub.photo-gallery #main .wrap .col ul li a {
  color: #636466;
  text-transform: uppercase;
  text-decoration: none;
}

.sub.photo-gallery #main .wrap .col ul li a:hover {
  text-decoration: underline;
}

a {
  color: #A263A8;
  text-decoration: none;
}

a:hover,
a:focus {
  text-decoration: underline;
}

#main p {
  line-height: 19px;
}

.sub #main p {
  margin: 15px 0;
}

p.strong {
  color: #000;
  font-weight: normal;
  font-size: 12px;
  margin: 15px 0;
}

#container {
  width: 1100px;
  margin: 0 auto;
  position: relative;
}

#content {
  background: #e4e4e5 url("/img/bg_main.png") repeat-y center center;
  width: 1100px;
  min-height: 476px;
}

.store-map #content {
  background-color: #fff;
  width: 1046px;
  margin: 0 auto;
}

#main {
  position: relative;
  width: 1046px;
  margin: 0 auto;
  /* height: 396px; */
  /* padding-top: 80px; */
}

.sub #main {
  padding-left: 43px;
  padding-right: 43px;
  width: 960px;
  height: auto;
}

