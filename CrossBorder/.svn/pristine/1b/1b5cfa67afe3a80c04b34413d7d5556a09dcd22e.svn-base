  float: left;
  list-style: none;
}

ul.nav li {
  float: left;
  margin-right: 6px;
}

ul.nav li a {
  float: left;
}

ul.nav.buttons {
  float: none;
}

ul.nav.buttons li.social {
  width: 20px;
  height: 20px;
  margin: 0 6px 0 0;
}

  ul.nav.buttons li.social a {
    margin: 0;
  }
  
  ul.nav.buttons li.social.tw a {
    background-position: -28px 0;
  }
  
ul.subnav.nav {
  margin-top: -20px;
  margin-bottom: 30px;
  float: none;
}

 ul.subnav.nav li {
   margin-right: 20px;
 }

    ul.subnav.nav li a {
      text-transform: uppercase;
      color: #A263A8;
      font-weight: bold;
      font-size: 11px;
    }


/* MAIN NAVIGATION */

#nav_main {
  position: absolute;
  z-index: 900;
  width: 990px;
  background: #fff;
  margin: 0 50px;
  height: 43px;
  top: 15px;
  padding: 0 5px;
  box-shadow: 0px 0px 5px #000;
}

  #nav_main ul {
    height: 43px;
    float: left;
    list-style: none;
    background: url("../img/bg_nav.png") repeat-x 0 0;
    padding: 0 10px;
    width: 970px;
    z-index: 950;
  }
  
    #nav_main ul li {
      float: left;
      height: 43px;      
      text-align: center;
      width: auto;
    }
    
      #nav_main ul li a {
        line-height: 44px;
        text-transform: uppercase;
        font-size: 12px;
        padding: 0 7px;
        height: 43px;
        display: block;
        color: #fff;
      }
      
  
      
      #nav_main ul li#nav_shopping a,
      #nav_main ul li#nav_dining a,
      #nav_main ul li#nav_lifestyle a,
      #nav_main ul li#nav_hotel a,
      #nav_main ul li#nav_offices a,
      #nav_main ul li#nav_living a {
        /* cursor: default; */
        text-decoration: none;
      }
      
      #nav_main ul li a:hover,
      #nav_main ul li a:focus,
      #nav_main ul li a.active {
        /*background: url("/img/bg_nav_active.png") repeat-x 0 0;
                text-decoration: none;*/
      }
      
      /* Dropdowns */
      
      #nav_main ul li .dropdown {
        position: absolute;
        left: -9999em;
        width: 950px;
        z-index: 899;
        background: #fff url("/img/bg_dropdown.png") repeat-x 0 0;      
        box-shadow: 0px 3px 5px #000;
        padding: 20px 20px;
        filter: alpha(opacity=95);
        opacity: 0.95;  
        -ms-filter:"progid:DXImageTransform.Microsoft.Alpha(opacity=95)";
      }
      
      #nav_main ul li:hover {
        cursor: default;
      }
      
      #nav_main ul li:hover .dropdown {
        left: 5px;
        top: auto;
      }
      
        #nav_main ul li .dropdown .col {
          width: 188px;
          float: left;
          border-right: 2px solid #ccc;
          text-align: left;
        }
        
        /* Shopping dropdown */
        
        #nav_main ul li#nav_shopping .dropdown {
          background-color: #78297e;
        	/* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(120, 41, 126);
        	/* RGBa with 0.6 opacity */
        	background: rgba(120, 41, 126, 0.95);          	
          }
        }
        
        #nav_main ul li#nav_shopping .dropdown .col {
          width: 188px;
        }
        
        #nav_main ul li#nav_shopping .dropdown .col:first-child {
          width: 150px;
        }
        
        /* Dining dropdowns */
        
        #nav_main ul li#nav_dining .dropdown {
          background-color: #78297e;
          /* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(120, 41, 126);
        	/* RGBa with 0.6 opacity */
        	background: rgba(120, 41, 126, 0.95);
        }        
        
        #nav_main ul li#nav_dining .dropdown .col {
          width: 200px;
        }
        
        #nav_main ul li#nav_dining .dropdown .col:first-child {
          width: 128px;
        }
        
        /* Lifestyle dropdown */
        
        #nav_main ul li#nav_lifestyle .dropdown {
          background-color: #ed217c;
          /* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(237, 33, 124);
        	/* RGBa with 0.6 opacity */
        	background: rgba(237, 33, 124, 0.95);
        }        
        
        #nav_main ul li#nav_lifestyle .dropdown .col {
          width: 250px;
        }
        
        #nav_main ul li#nav_lifestyle .dropdown .col:first-child {
          width: 230px;
        }
        
        /* Hotel dropdown */
        
        #nav_main ul li#nav_hotel .dropdown {
          background-color: #fbc510;
          /* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(251, 197, 16);
        	/* RGBa with 0.6 opacity */
        	background: rgba(251, 197, 16, 0.95);
        }
        
        #nav_main ul li#nav_hotel .dropdown .col {
          /*width: 250px;*/ width: 300px;
        }
        
        #nav_main ul li#nav_hotel .dropdown .col:first-child {
          width: 190px;
        }
        
        /* Living dropdown */
        
        #nav_main ul li#nav_living .dropdown {
          background-color: #b2cf8f;
          /* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(178, 207, 143);
        	/* RGBa with 0.6 opacity */
        	background: rgba(178, 207, 143, 0.95);
        }
        
        #nav_main ul li#nav_living .dropdown .col {
          width: 250px;
        }
        
        #nav_main ul li#nav_living .dropdown .col:first-child {
          width: 190px;
        }
        
        /* Offices dropdown */
        
        #nav_main ul li#nav_offices .dropdown {
          background-color: #999999;
        	/* Fallback for web browsers that doesn't support RGBa */
        	background: rgb(153, 153, 153);
        	/* RGBa with 0.6 opacity */
        	background: rgba(153, 153, 153, 0.95);          	
          }
        }
        
        #nav_main ul li#nav_offices .dropdown .col {
          width: 188px;
        }
        
        #nav_main ul li#nav_offices .dropdown .col:first-child {
          width: 130px;
        }
        
        
        #nav_main ul li .dropdown .col:first-child strong {
          font-size: 25px;
          color: #fff;
          font-weight: normal;
          text-transform: uppercase;
        }
        
       /* #nav_main ul li .dropdown .col:last-child,*/
        #nav_main ul li .dropdown .col:first-child {
          border-right: none;
        }
        
        #nav_main ul li .dropdown .col:first-child {
          border-right: none;
        }
        
        #nav_main ul li .dropdown .col.start {
          border-left: 2px solid #ccc;          
          width: 186px;
        }
        
          #nav_main ul li .dropdown .col ul,
          #nav_main ul li .dropdown .col ul li,
          #nav_main ul li .dropdown .col ul li a {
            float: none;
            background: none;
            text-align: left;
          }                              
          
          #nav_main ul li .dropdown .col ul {
            height: auto;
            width: auto;
          }
          
            #nav_main ul li .dropdown .col ul li {
              height: 20px;
              width: auto;                         
            }
          
              #nav_main ul li .dropdown .col ul li a {
                display: inline;
                text-transform: none;
                color: #fff;
                font-size: 13px;
                line-height: 21px;
                cursor: pointer !important;
              }
              
              #nav_main ul li .dropdown .col ul li a:hover {
                text-decoration: underline !important;
              }


/* SEARCH FORM */

