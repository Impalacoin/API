}

#masthead ul li {
  float: left;
  margin-top: 2px;
}

#masthead ul li.social {
  margin-top: 0;
}

#masthead ul li a {
  float: left;
}

#masthead ul li a.btn {
  margin-left: 7px;
}


/* NAVS */

