  font-family:Arial, Helvetica, sans-serif; font-size:18px; font-weight:none; color:#5973a8; padding:15px 0 5px 0; border-bottom:1px solid #e9e9e9; margin:0 20px;
}

.style-bold {color: #555555; font-size: 12px; font-weight: bold; padding: 3px 0;
}
.style-light {color:#777777;
}

.style-info {font-size:12px; width:95%; padding:10px; padding-left:10px; background-color:#d3e8fb; border:1px solid #61add8; background-image:url(ico_info.png); background-position:left top; background-repeat:no-repeat;}
.style-length {font-size:12px; width:100%; padding:10px; padding-left:10px; }
.style-width{ margin-left:10px; width:250px}


