public class ClientAccountBalanceByCountry extends AccountBalance {

	private String countryUuid;

	/**
	 * Default constructor
	 */
	public ClientAccountBalanceByCountry() {
		super();

		countryUuid = "";

	}

	public String getCountryUuid() {
		return countryUuid;
	}

	public void setCountryUuid(String countryUuid) {
		this.countryUuid = countryUuid;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("ClientAccountBalanceByCountry [getUuid()=");
		builder.append(getUuid());
		builder.append(", getBalance()=");
		builder.append(getBalance());
		builder.append(", getAccountUuid()=");
		builder.append(getAccountUuid());
		builder.append(", countryUuid=");
		builder.append(countryUuid);
		builder.append("]");
		return builder.toString();
	}

	

}
