        End If
        If Trim(TextBox1.Text) <> "" Then
            With AccessDBCommnd
                .Connection = AccessDBConnection
                .CommandType = CommandType.Text
                .CommandText = "Delete from recon_entity where entity_name = '" + Trim(TextBox1.Text) + "'"
                .ExecuteNonQuery()

            End With
        Else
