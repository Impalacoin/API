Private Sub Command1_Click()
Dim sqlString As String
Dim sqlString2 As String
On Error GoTo ErrorTrap
sqlString = "INSERT INTO [dbo].[em_term] ([id],[short_name],[term_active],[support_team],[worst_event_severity]" & _
           ",[card_acceptor],[participant_id],[pos_geographic_data],[sponsor_bank],[pos_data_code],[serial_nr]" & _
           ",[date_deployed],[last_message_type],[last_message_time],[status],[hardware_config],[value_bars]" & _
           ",[miscellaneous],[security_team],[media_team],[supplies_team],[term_mode],[last_tran_msg_time])"
sqlString2 = "Values(" & Trim(Text2.Text) & "'," & _
           "'" & Trim(Text1.Text) & "'," & _
           "," & _
           "," & _
           "," & _
           "'" & Combo5.List(Combo5.ListIndex) & "'," & _
           "," & _
           "," & _
           "'" & Combo3.List(Combo3.ListIndex) & "'," & _
           "," & _
           "'" & Trim(Text3.Text) & "'," & _
           "'" & Trim(Text11.Text) & "'," & _
           "," & _
           "," & _
           "," & _
           "'" & Trim(Text8.Text) & Space(20 - Len(Text8.Text)) & Trim(Text9.Text) & Space(15 - Len(Text9.Text)) & Combo3.List(Combo3.ListIndex) & "'," & _
           "," & _
           "'" & Combo4.List(Combo4.ListIndex) & "'," & _
           "," & _
           "," & _
           "," & _
           "1," & _
           ","
DbCon.Execute sqlString & sqlString2
Exit Sub
ErrorTrap:

End Sub

