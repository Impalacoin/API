            'reCset.Open("select distinct(class_year) from class order by class_year asc", UniversalHostConnection)
            ''ComboBox2.Items.Clear()
            'Do While reCset.EOF = False
            '    ComboBox2.Items.Add(UCase(reCset.Fields(0).Value))
            '    reCset.MoveNext()
            'Loop
            'reCset.Close()


