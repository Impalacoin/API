<script type="text/javascript">
    var month = 01;
    var year = 2014;
    var cal = "";

    //render_calendar(cal,month,year);

    function render_calendar(cal,month,year) {
	$("#mini_cal_wrapper").css("background", "url('/img/loading2.gif') no-repeat center center");
        $.post(
            "http://citycentrehouston.com//events/mini_cal",
            { cal: cal, month : month, year : year },
            function(str) {
                $('#mini_cal').html(str).fadeIn('fast');
		$("#mini_cal_wrapper").css("background", "none");
            }
        );
    }

    $("a.calMonthLinks").live("click", function(e){
        //$('#mini_cal').hide();
	$('#mini_cal').fadeOut('fast');
        month = $(this).attr('alt');
        year = $(this).attr('rel');
        cal = $(this).attr('data-type');
        render_calendar(cal,month,year);
        e.preventDefault();
    });


</script>









