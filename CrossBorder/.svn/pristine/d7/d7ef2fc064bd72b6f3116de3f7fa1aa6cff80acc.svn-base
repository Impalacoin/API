        onLoad : function(){

           var Paper = this.getData().R;
           var _data = this.getData();
           var mapsvg = this;

           _data.groups = {};

           // Init (set groups, colors)
           Paper.forEach(function(el){

               if(el.id == undefined || el.id=='') return;

               if(el.id.toString().substring(0,1) == 'r'){

                    var bbox = el.getBBox();
                    var c = {x: bbox.x + (bbox.width/2), y: bbox.y + (15)};

                    if(el._data && el._data.group){
                        
                        el.hover_attr.fill = mapsvg_groups[el._data.group]
                        el.selected_attr.fill = mapsvg_groups[el._data.group]

                        // Create a reference for group if it doesn't exist yet
                        if(_data.groups[el._data.group] === undefined)
                            _data.groups[el._data.group] = [];
                        // Add element into a group reference
                        _data.groups[el._data.group].push(el);
                    }

                    // Write pop-up position to room's data property
                    el.data({'popx': c.x , 'popy' : c.y});
               }
           });
         
           var selected_group = '';

           // Deselect group function
           mapsvg.deselect = function(){
                if(typeof _data.selected_id == 'object'){
                    for(var i in _data.selected_id){
                        var selectedRegion = _data.R.getById(_data.selected_id[i]);
                        selectedRegion.attr(selectedRegion.default_attr);                            
                    }                        
                }else if(_data.selected_id){
                    var selectedRegion = _data.R.getById(_data.selected_id);
                    selectedRegion.attr(selectedRegion.default_attr);                                                    
                }                        
           };
            
           // Select group function
           mapsvg.groupSelect = function(group){
            
                mapsvg.deselect();
                mapsvg.hidePopover();
                
                _data.options.multiSelect = true;
                _data.selected_id = [];

                // select group
                for(i in _data.groups[group])
                    mapsvg.selectRegion(_data.groups[group][i].id);

                _data.options.multiSelect = false;        
                selected_group = group;
           };
           
           mapsvg.cityCentreSelectRoom = function(id){
				var r = mapsvg.getData().R.getById(id);

                if(r._data.group)
                    mapsvg.groupSelect(r._data.group);
                
				// Show pop-up message
				var pop_pos = [
					r.data('popx'),
					r.data('popy')
				];
                
                // !!!! REMOVE 2 LINES BELOW TO BE ABLE TO SHOW YOUR OWN POP-UPS...
                // var pop = 'Room ID: <b>'+r.id+'</b>';
                // mapsvg.showPopover(null,pop, pop_pos);
                
                // !!!! ...AND UNCOMMENT THE LINE BELOW
                
                console.log(this);
                console.log(r._data);
                mapsvg.showPopover(null, r._data.popover, pop_pos);
				 
                                                
                r.attr({fill: '#ff0000'});
           };
           
           $('.mapsvg-select-group').on('click', function(e){
                e.preventDefault();
                mapsvg.groupSelect($(this).attr('data-group'));
           });
           
           $('.mapsvg-select-room').on('click', function(e){
                e.preventDefault();                 
                mapsvg.cityCentreSelectRoom($(this).attr('data-room-id'));           
           });

        }
	});



