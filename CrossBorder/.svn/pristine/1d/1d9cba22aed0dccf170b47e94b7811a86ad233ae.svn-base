Attribute VB_Name = "MenuForm"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Private Sub Form_Load()
'Me.Top = MainForm.Top + 1070
'Me.Left = MainForm.Left + 4300
Call Resize_Form(MenuForm, 600, 800)
End Sub

Public Sub Resize_Form(ByRef Form_Name As Form, Optional ByVal x_reso As Integer, Optional ByVal y_reso As Integer)
Dim No_of_Controls As Integer, I As Integer, J As Integer, xratio As Currency, yratio As Currency, AvgRatio As Currency
Dim org_width As Long, org_height As Long, New_Width As Long, New_Height As Long
Dim Org_Top As Long, Org_Left As Long, New_Top As Long, New_Left As Long
Dim Org_X1 As Long, Org_X2 As Long, Org_Y1 As Long, Org_Y2 As Long
Dim K As Integer, Temp_Width As Long, FontRatio As Currency
Dim GridColWidth(100) As Long

'Dim no_of_sub_controls As Integer
If Old_Reso_X <= 0 Then Old_Reso_X = 9600
If Old_Reso_Y <= 0 Then Old_Reso_Y = 7200

If x_reso > 0 Then Old_Reso_X = x_reso * 15
If y_reso > 0 Then Old_Reso_Y = y_reso * 15

If Screen.Width = Old_Reso_X And Screen.Height = Old_Reso_Y Then Exit Sub

xratio = Screen.Width / Old_Reso_X
yratio = Screen.Height / Old_Reso_Y

AvgRatio = (xratio + yratio) / 2
FontRatio = xratio
If yratio < FontRatio Then FontRatio = yratio
'FontRatio = FontRatio * 0.9
On Error Resume Next
With Form_Name

    org_width = .Width
    org_height = .Height
    Org_Top = .Top
    Org_Left = .Left

    New_Left = Org_Left * xratio
    New_Top = Org_Top * yratio
    New_Width = org_width * xratio
    New_Height = org_height * yratio

    .Width = New_Width
    .Height = New_Height
    .Top = New_Top
    .Left = New_Left

'Form Resized

No_of_Controls = .Count
If No_of_Controls <= 0 Then Exit Sub
For I = 0 To No_of_Controls - 1
    
    If TypeOf .Controls(I) Is Label Or TypeOf .Controls(I) Is CommandButton Or _
        TypeOf .Controls(I) Is TextBox Or TypeOf .Controls(I) Is RichTextBox Or _
        TypeOf .Controls(I) Is SButton Or _
        TypeOf .Controls(I) Is MainBtn Or _
        TypeOf .Controls(I) Is StatusBar Or _
        TypeOf .Controls(I) Is HScrollBar Or TypeOf .Controls(I) Is VScrollBar Or _
        TypeOf .Controls(I) Is PictureBox Or TypeOf .Controls(I) Is OptionButton Or _
        TypeOf .Controls(I) Is CheckBox Or TypeOf .Controls(I) Is Frame Or _
        TypeOf .Controls(I) Is Shape Or TypeOf .Controls(I) Is Picture Or _
        TypeOf .Controls(I) Is SSTab Or TypeOf .Controls(I) Is Image Or _
        TypeOf .Controls(I) Is MainBtn Or TypeOf .Controls(I) Is SButton Then
            GoSub Revise_Figures
            GoTo Skip_This
    End If
    
    If TypeOf .Controls(I) Is Line Then
        Org_X1 = .Controls(I).X1
        Org_X2 = .Controls(I).X2
        Org_Y1 = .Controls(I).Y1
        Org_Y2 = .Controls(I).Y2
        .Controls(I).X1 = Org_X1 * xratio
        .Controls(I).X2 = Org_X2 * xratio
        .Controls(I).Y1 = Org_Y1 * yratio
        .Controls(I).Y2 = Org_Y2 * yratio
        GoTo Skip_This
    End If
    
    If TypeOf .Controls(I) Is ComboBox Then
        org_width = .Controls(I).Width
        Org_Top = .Controls(I).Top
        Org_Left = .Controls(I).Left
        New_Height = org_height * yratio
        New_Width = org_width * xratio
        New_Left = Org_Left * xratio
        If (Org_Left < 0) Then New_Left = ((Org_Left + 75000) * xratio - 75000)
        New_Top = Org_Top * yratio
        .Controls(I).Top = New_Top
        .Controls(I).Width = New_Width
        .Controls(I).Left = New_Left
        .Controls(I).FontSize = .Controls(I).FontSize * FontRatio - 0.5
        GoTo Skip_This
    End If
    
'    If TypeOf .Controls(I) Is DTPicker Then
'        org_width = .Controls(I).Width
'        Org_Top = .Controls(I).Top
'        Org_Left = .Controls(I).Left
'        New_Height = org_height * yratio
'        New_Width = org_width * xratio
'        New_Left = Org_Left * xratio
'        If (Org_Left < 0) Then New_Left = ((Org_Left + 75000) * xratio - 75000)
'        New_Top = Org_Top * yratio
'        .Controls(I).Top = New_Top
'        .Controls(I).Width = New_Width
'        .Controls(I).Left = New_Left
'        .Controls(I).Font.Size = .Controls(I).Font.Size * FontRatio - 0.5
'        GoTo Skip_This
'    End If
    
 '======
     If TypeOf .Controls(I) Is MSFlexGrid Then
        .Controls(I).RowHeightMin = 260 * yratio
        org_width = .Controls(I).Width - 480
        org_height = .Controls(I).Height
        Org_Top = .Controls(I).Top
        Org_Left = .Controls(I).Left
        
        New_Width = org_width * xratio
        New_Height = org_height * yratio
        New_Top = Org_Top * yratio
        New_Left = Org_Left * xratio
        If (Org_Left < 0) Then New_Left = ((Org_Left + 75000) * xratio - 75000)
        
        K = .Controls(I).Cols
        For J = 0 To K - 1
            GridColWidth(J) = .Controls(I).ColWidth(J)
        Next J
        
        .Controls(I).Width = New_Width + 480 * xratio
        .Controls(I).Height = New_Height
        .Controls(I).Top = New_Top
        .Controls(I).Left = New_Left
        .Controls(I).FontSize = .Controls(I).FontSize * FontRatio - 0.5
        
step_1:

        If GridColWidth(K - 1) > 0 Then GoTo Continue_1
        K = K - 1
        GoTo step_1
        
Continue_1:
       
        Temp_Width = 0
        For J = 0 To K - 2
             .Controls(I).ColWidth(J) = (GridColWidth(J) * xratio) + 0
             Temp_Width = Temp_Width + .Controls(I).ColWidth(J)
        Next J
        If New_Width - Temp_Width > 0 Then .Controls(I).ColWidth(K - 1) = New_Width - Temp_Width
        GoTo Skip_This
    End If

 '=====
    If TypeOf .Controls(I) Is UcDataField Then
        org_width = .Controls(I).Width
        org_height = .Controls(I).Height
        Org_Top = .Controls(I).Top
        Org_Left = .Controls(I).Left
     
        New_Width = org_width * xratio
        New_Height = org_height * yratio
        New_Top = Org_Top * yratio
        New_Left = Org_Left * xratio
        If (Org_Left < 0) Then New_Left = ((Org_Left + 75000) * xratio - 75000)
    
        .Controls(I).Width = New_Width
        .Controls(I).Height = New_Height
        .Controls(I).Top = New_Top
        .Controls(I).Left = New_Left
        .Controls(I).Set_Font_Size = .Controls(I).Set_Font_Size * FontRatio - 0.5
        GoTo Skip_This
    End If
    GoTo Skip_This
    
Revise_Figures:

    org_width = .Controls(I).Width
    org_height = .Controls(I).Height
    Org_Top = .Controls(I).Top
    Org_Left = .Controls(I).Left
 
    New_Width = org_width * xratio
    New_Height = org_height * yratio
    New_Top = Org_Top * yratio
    New_Left = Org_Left * xratio
    If (Org_Left < 0) Then New_Left = ((Org_Left + 75000) * xratio - 75000)

    'If Not (TypeOf .Controls(I) Is Picture Or TypeOf .Controls(I) Is StatusBar) Then .Controls(I).Width = New_Width
    If Not TypeOf .Controls(I) Is StatusBar Then .Controls(I).Width = New_Width
    .Controls(I).Height = New_Height
    .Controls(I).Top = New_Top
    'If Not (TypeOf .Controls(I) Is Picture Or TypeOf .Controls(I) Is StatusBar) Then .Controls(I).Left = New_Left
    If Not TypeOf .Controls(I) Is StatusBar Then .Controls(I).Left = New_Left
    If Not (TypeOf .Controls(I) Is Shape Or TypeOf .Controls(I) Is Image Or TypeOf .Controls(I) Is HScrollBar Or TypeOf .Controls(I) Is VScrollBar) Then .Controls(I).Font.Size = .Controls(I).Font.Size * FontRatio - 0.5
    
        If TypeOf .Controls(I) Is SButton Or TypeOf .Controls(I) Is MainBtn Then
    ''        .Controls(I).FontSize = .Controls(I).FontSize * FontRatio - 0.5
    '        .Controls(I).Picture1.Width = .Controls(I).Picture1.Width * XRatio
    '        .Controls(I).Picture1.Height = New_Height
    '        .Controls(I).Picture1.Top = New_Top
    '        .Controls(I).Picture1.Left = New_Left
    '        .Controls(I).Label1.Width = New_Width - .Controls(I).Picture1.Width
    '        .Controls(I).Label1.Height = New_Height
    '        .Controls(I).Label1.Top = New_Top
    '        .Controls(I).Label1.Left = .Controls(I).Picture1.Left
    '        .Controls(I).Label1.Refresh
    '        .Controls(I).Label1.Font.Size = .Controls(I).Label1.Font.Size * FontRatio - 0.5
        End If
    Return
    
Skip_This:

Next I
End With
Exit Sub

End Sub

