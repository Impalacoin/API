function chkfrm(){
  var aba = payment_form.ach_routing_number.value;
  var valid = "0123456789";
  var len = aba.length;
  var bNum = true;
  var iABA = parseInt(aba);
  var sABA = aba.toString();
  var iTotal = 0;
  var bResult = false;
  var temp;

  for (var j=0; j<len; j++) {
  temp = "" + aba.substring(j, j+1);
  if (valid.indexOf(temp) == "-1") bNum = false;
  }

  if(len !=0) {  
    if(len != 9) {
    } else {
      for (var i=0; i<len; i += 3) {
        iTotal += parseInt(sABA.charAt(i),     10) * 3
          +  parseInt(sABA.charAt(i + 1), 10) * 7
          +  parseInt(sABA.charAt(i + 2), 10);
        }
		
    if (iTotal != 0 && iTotal % 10 == 0){
      bResult = true;
    } else {
      bResult = false;
    }
  }
  } else {
  }
    
  if (!bResult) {
   alert("Please enter a valid routing number");
     return false;
  } else {
  return true;

  }

}

